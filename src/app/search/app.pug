extends /layouts/layout
include /components/common/mTitle
include /components/common/mPagination
include /components/searchHead
include /components/stateBar
include /components/filter
include /components/list

block append contents
  section.searchHeadArea
    +searchHead('typeApp', false, '마케팅 입력검색어', '배너', '마케팅', '고객지원', '유료', '브론즈')

  section.searchArea.gridSet
    - var filterAppData = require('./src/js/data/filterApp.js');
    div.wFilter
      div.filterWrap
        +filter(filterAppData)

    div.wFilterResult
      +stateBar(false)
      - var searchAppData = require('./src/js/data/searchApp.js');
      +list(searchAppData, 'typeApp')
      +mPagination('typeMore', false)

block append body
  script(src="/js/ui/search.js")
